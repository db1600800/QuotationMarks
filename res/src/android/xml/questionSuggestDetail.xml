<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="vertical" >
<DivLayout_Vertical  android:layout_width="fill_parent"  android:layout_height="fill_parent"    android:background="#FFFFFF"   android:orientation="vertical"  android:gravity="">
<DivLayout_Vertical  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="vertical"  android:gravity="">
<TextView   android:id="@+id/questionMsgTextView" android:layout_width="fill_parent"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="需求豆腐饭d"   android:textSize="12sp" android:textColor="#838383"   android:gravity="center" >
</TextView>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="horizontal"  android:gravity="">
<TextView   android:id="@+id/modelTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="需求管理模块"   android:textSize="20sp" android:textColor="#4C4C4C"   android:gravity="center" >
</TextView>
</DivLayout_Horizon>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="horizontal"  android:gravity="">
<TextView   android:id="@+id/applicationTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="企业管理平台"   android:textSize="16sp" android:textColor="#ABABAB"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/lineTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="|"   android:textSize="16sp" android:textColor="#ABABAB"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/questionTypeTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="故障及问题"   android:textSize="16sp" android:textColor="#ABABAB"   android:gravity="center" >
</TextView>
</DivLayout_Horizon>
</DivLayout_Vertical>
<DivLayout_Vertical  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="vertical"  android:gravity="">
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#ffffff"  android:orientation="horizontal"  >
<ImageView   android:id="@+id/fileimgImageView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp"   android:background="@drawable/fileimg"  >
</ImageView>
<Button   android:id="@+id/fileurlButton" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="10dp" android:layout_marginRight="10dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="guangldong"   android:textSize="16sp" android:textColor="#7878D3"   android:background="背景颜色"  >
</Button>
</DivLayout_Horizon>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="horizontal"  android:gravity="">
<TextView   android:id="@+id/feedBackTitleTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="反馈人"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/feedBackNameTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="张三"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/line1TextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="|"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/FeedBackPhoneTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="13343"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/line3TextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="|"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/FeedBackOrgTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="广州"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center" >
</TextView>
</DivLayout_Horizon>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="horizontal"  android:gravity="">
<TextView   android:id="@+id/FeedBackTimeTitleTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="反馈时间:"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/FeedBackTimeTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="2018"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center" >
</TextView>
</DivLayout_Horizon>
</DivLayout_Vertical>
<DivLayout_Vertical  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="vertical"  android:gravity="">
<EditText   android:id="@+id/dealMsgEditText" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:hint="处理意见"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center"   android:singleLine="true"   android:maxLength="40"  >
</EditText>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="horizontal"  android:gravity="">
<EditText   android:id="@+id/dealNameEditText" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:hint="处理人"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center"   android:singleLine="true"   android:maxLength="40"  >
</EditText>
<EditText   android:id="@+id/dealPhoneEditText" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:hint="电话"   android:textSize="16sp" android:textColor="#4C4C4C"   android:gravity="center"   android:singleLine="true"   android:maxLength="40"  >
</EditText>
</DivLayout_Horizon>
</DivLayout_Vertical>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="horizontal"  android:gravity="">
<Button   android:id="@+id/sumbitButton" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="10dp" android:layout_marginRight="10dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="提交"   android:textSize="16sp" android:textColor="#FFFFFF"   android:background="#169BD5"  >
</Button>
</DivLayout_Horizon>
</DivLayout_Vertical>
</LinearLayout>




