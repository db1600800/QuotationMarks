<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/widget34"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/order_bg" >

    <RelativeLayout
        android:id="@+id/buy_shoppingcar_productlist_item_img_layout"
        android:layout_width="100dp"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" >

        <CheckBox
            android:id="@+id/buy_shoppingcar_productlist_item_checkBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true" />

        <ImageView
            android:id="@+id/ProductShoppingcarStoreUp_ProductPic_select_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/bg_account_user_address" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/buy_shoppingcar_productlist_item_productmsg_layout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_centerInParent="true"
        android:layout_toLeftOf="@+id/buy_shoppingcar_productlist_item_btn_layout"
        android:layout_toRightOf="@+id/buy_shoppingcar_productlist_item_img_layout" >

        <RelativeLayout
            android:id="@+id/buy_shoppingcar_productlist_item_productname_layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/ProductShoppingcarStoreUp_ProductName_select_textview"
                android:layout_width="73dp"
                android:layout_height="18dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="产品名字" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/buy_shoppingcar_productlist_item_productstype_layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/buy_shoppingcar_productlist_item_productname_layout" >

            <Button
                android:id="@+id/buy_shoppingcar_productlist_decrement_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:text="数量减一" />

            <EditText
                android:id="@+id/ProductShoppingcarStoreUp_BuyNumber_select_edittext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
               
                 />

            <Button
                android:id="@+id/buy_shoppingcar_productlist_increase_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
               
                android:text="数量加一" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/buy_shoppingcar_productlist_item_productprice_layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/buy_shoppingcar_productlist_item_productstype_layout" >

            <TextView
                android:id="@+id/ProductShoppingcarStoreUp_ProductIntegrate_select_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:text="产品点数" />

            <TextView
                android:id="@+id/ProductShoppingcarStoreUp_ProductPrice_select_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="产品价格" />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/buy_shoppingcar_productlist_item_btn_layout"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="false" >

        <Button
            android:id="@+id/ProductShoppingcarStoreUp_ProductId_isInShoppingCar_true_update_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="购物车" />

        <Button
            android:id="@+id/ProductShoppingcarStoreUp_ProductId_isInShoppingCar_false_update_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ProductShoppingcarStoreUp_ProductId_isInShoppingCar_true_update_btn"
            android:layout_centerHorizontal="true"
            android:text="取消" />
    </RelativeLayout>

</RelativeLayout>